'use strict';

/* Controllers */


function MyCtrl1() {}
MyCtrl1.$inject = [];


function MyCtrl2() {
}
MyCtrl2.$inject = [];

function LocationController($resource) {
	var Location = $resource('/locations/:_id');
	var loc = new Location({});
	$scope.loc = loc;
 
  $scope.update = function(user) {
    loc.$save();
  };
 
  $scope.reset = function() {
    $scope.loc = {};
  };
 
  $scope.isUnchanged = function(user) {
    return angular.equals(user, {});
  };
 
  $scope.reset();
}
LocationController.$inject = ['$resource'];
